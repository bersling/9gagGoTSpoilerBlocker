{
  "manifest_version": 2,

  "name": "9gag GoT Spoiler Blocker",
  "description": "Block Game of Thrones Spoilers on 9gag",
  "version": "1.2",

  "page_action": {
    "default_icon": "gotblock.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "*://*.9gag.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.9gag.com/*"],
      "js": ["hello.js"]
    }
  ]
}